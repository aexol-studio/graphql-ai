{
  "compilerOptions": {
    "skipLibCheck": true,
    "target":"es2020",
    "module": "esnext",
    "declaration": true,
    "moduleResolution": "node",
    "outDir": "./lib",
    "rootDir": "./",
    "strict": true,
    "strictNullChecks": true,
    "esModuleInterop": true
  },
  "include": ["./**/*"],
  "exclude": ["node_modules","lib"]
}
